func Membership(X, Y, Z : Integer) is
    if X in Y..Z then
	Println(X | " in " | Y | ".." | Z);
    else
	Println(X | " not in " | Y | ".." | Z);
    end if;
end func Membership;

func main(Args : Basic_Array<Univ_String>) is
   Membership (1, 2, 4);
   Membership (2, 2, 4);
   Membership (3, 2, 4);
   Membership (4, 2, 4);
   Membership (5, 2, 4);
end func main;
